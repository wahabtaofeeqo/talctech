<%- contentFor('body') %>
	<div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">
        	<%= title %>
        </h1>
    </div>

	<div class="card">
		<div class="card-body">
			<div class="table-responsive">
	        	<table class="table table-striped" id="dataTable" width="100%" cellspacing="0">
	        		<thead>
	        			<tr>
		        			<th>#</th>
		        			<th>Type</th>
		        			<th>Location</th>
		        			<th>Price</th>
		        			<th>Action</th>
		        		</tr>
	        		</thead>

	        		<tbody>
	        			<% for(let i = 0; i < properties.length; i++) {%>
	        				<tr>
	        					<td><%= (i + 1) %></td>
	        					<td><%= properties[i].property_type %></td>
	        					<td><%= properties[i].city %></td>
	        					<td><%= properties[i].price %></td>
	        					<td>
	        						<a href="/properties/<%= properties[i].id %>" class="btn btn-sm btn-info" target="_blank">View</a>
	        					</td>
	        				</tr>
	        			<% } %>
	        		</tbody>
	        	</table>
	        </div>
		</div>
	</div>