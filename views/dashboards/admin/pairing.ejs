<%- contentFor('body') %>

	<div class="card">
		<div class="card-body">
			<div class="table-responsive">
	        	<table class="table table-striped" id="dataTable" width="100%" cellspacing="0">
	        		<thead>
	        			<tr>
		        			<th>#</th>
		        			<th>Name</th>
		        			<th>Email</th>
		        			<th>Phone</th>
		        			<th>Action</th>
		        		</tr>
	        		</thead>

	        		<tbody>
	        			<% for(let i = 0; i < tenants.length; i++) {%>
	        				<tr>
	        					<td><%= (i + 1) %></td>
	        					<td><%= tenants[i].name %></td>
	        					<td><%= tenants[i].email %></td>
	        					<td><%= tenants[i].phone %></td>
	        					<td>
	        						<a href="/admin/pairing/<%= tenants[i].id %>" class="btn btn-sm btn-warning">
	        							Check Matches
	        						</a>
	        					</td>
	        				</tr>
	        			<% } %>
	        		</tbody>
	        	</table>
	        </div>
		</div>
	</div>