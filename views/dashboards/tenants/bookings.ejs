<%- contentFor('body') %>
	<div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Bookings</h1>
    </div>

	<div class="card">
		<div class="card-body">
			<div class="table-responsive">
	        	<table class="table table-striped" id="dataTable" width="100%" cellspacing="0">
	        		<thead>
	        			<tr>
		        			<th>#</th>
		        			<th>Property Type</th>
		        			<th>Offer Type</th>
		        			<th>Location</th>
		        			<th>Action</th>
		        		</tr>
	        		</thead>

	        		<tbody>
	        			<% for(let i = 0; i < bookings.length; i++) {%>
	        				<tr>
	        					<td><%= (i + 1) %></td>
	        					<td><%= bookings[i].property.property_type %></td>
	        					<td><%= bookings[i].property.offer_type %></td>
	        					<td><%= bookings[i].property.offer_type %></td>
	        					<td>
	        						<button class="btn btn-sm btn-info btn-survey" data-id="<%= bookings[i].id %>">
	        							Take Survey
	        						</button>
	        					</td>
	        				</tr>
	        			<% } %>
	        		</tbody>
	        	</table>
	        </div>
		</div>
	</div>