<!doctype html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Taltech</title>
    <link rel="icon" href="/assets/img/logo.jpg">
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">

    <link rel="stylesheet" href="/assets/css/animate.css">

    <link rel="stylesheet" href="/assets/css/owl.carousel.min.css">

    <link rel="stylesheet" href="/assets/css/themify-icons.css">

    <link rel="stylesheet" href="/assets/css/flaticon.css">

    <link rel="stylesheet" href="/assets/css/magnific-popup.css">

    <link rel="stylesheet" href="/assets/css/slick.css">
    <link rel="stylesheet" href="/assets/css/all.css">

    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="stylesheet" href="/assets/css/nav.css">
</head>

<body>

    <header class="main_menu">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-dark">
                        <a class="navbar-brand" href="/">
                            <img src="/assets/img/logo.jpg" style="width: 100px;" alt="logo">
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse"
                            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse main-menu-item justify-content-end"
                            id="navbarSupportedContent">
                            <ul class="navbar-nav">
                                <li class="nav-item ">
                                    <a class="nav-link" href="/" style="color: #fff;">Home</a>
                                </li>
                                <li class="nav-item active">
                                    <a class="nav-link" href="/about">About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/feature">Featured Properties</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/rentals">Rentals</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/contact">Contact</a>
                                </li>
                            </ul>

                            <% if(!user) {%>
                                <a href="/signup" class="btn btn-primary">Sign Up</a>
                                <a href="/login" class="btn btn-outline-primary">Login</a>
                            <% } else { %>
                                <a href="/dashboard" class="btn btn-outline-primary">Dashboard</a>
                            <% } %>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>



    <section class="breadcrumb breadcrumb_bg-rental">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner text-center">
                        <div class="breadcrumb_iner_item">
                            <h2>Rental Properties</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <!-- FEATURED PROPERTIES -->
    <section class="blog_part section_padding">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-xl-5">
                    <div class="section_tittle text-center">
                        <h2>RENTAL PROPERTIES</h2>
                        <p style="color: black">Browse houses and flats for sale and to rent in your area</p>
                    </div>
                </div>
            </div>

            <div class="row">
                    <!-- Properties -->
                    <% for(let i = 0; i < properties.length; i++) { %>

                        <div class="col-sm-6 col-lg-4 col-xl-4 d-sm-block mb-4">
                            <div class="single-home-blog">
                                <div class="card">
                                    
                                  <img src="uploads/shortproperties/<%= properties[i].image_front %>" class="card-img-top" alt="Property">
                                    
                                    <div class="card-body">
                                        <span><i class="fas fa-map-marker-alt mr-2"
                                                style="color: #C17267;"></i><%= properties[i].city %></span> |
                                        <span><%= properties[i].neighbourhood %></span>
                                        <hr>
                                        <div class="row">
                                            <div class="col-12">
                                                <i class="fas fa-money-bill-alt mr-2"
                                                    style="color: #C17267;"></i>(â‚¦)<%= properties[i].price %>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-12">
                                                <i class="fas fa-list-alt mr-2" style="color: #C17267;"></i> Type:
                                                <%= properties[i].property_type %>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row">
                                            <div class="col-6">
                                                <i class="fas fa-bed  mr-2" style="color: #C17267;"></i><%= properties[i].bedrooms %>
                                            </div>
                                            <div class="col-6">
                                                <i class="fas fa-bath  mr-2" style="color: #C17267;"></i><%= properties[i].bathrooms %>
                                            </div>
                                        </div>
                                        <hr>
                                        <a href="/booking/<%= properties[i].id %>" class="btn btn-primary"
                                            style="color: #fff; width: 100%; padding: 10px; background-color: #C17267;">View and Book
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>

                    <% } %>
            </div>

            <div class="row d-none">
                <div class="col-sm-6 col-lg-4 col-xl-4">
                    <div class="single-home-blog">
                        <div class="card">
                            <img src="/assets/img/20.jpg" class="card-img-top" alt="blog">
                            <div class="card-body">
                                <a href="#">Park View Estate</a> | <span><i class="fas fa-map-marker-alt"
                                        style="color: #C17267;"></i> Ikoyi, Lagos</span>
                                <a href="#">
                                </a>
                                <hr>
                                <ul>
                                    <li> <i class="fas fa-th-large"></i> 800 Sq foot</li>
                                    <li> <i class="fas fa-car-side"></i>1 Garage</li>
                                    <hr>
                                    <li> <i class="fas fa-bed"></i>4 Bedrooms</li>
                                    <li> <i class="fas fa-sink"></i>3 Bathrooms</li>
                                    <hr>
                                    <li> <i class="fas fa-user"></i> Baba Salalal</li>
                                    <li> <i class="far fa-clock"></i></i> 1 day ago</li>
                                </ul>
                                <br>
                                <a href="#" class="btn btn-primary"
                                    style="color: #fff; width: 100%; padding: 10px;">VIEW</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- pagination -->
            <nav aria-label="Page navigation example d-none" id="pagination">
                <ul class="pagination">
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Previous">
                            <span aria-hidden="true">&laquo;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                    <li class="page-item">
                        <a class="page-link" href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </section>

    <!-- footer -->
    <footer class="footer-area">
        <div class="container">
            <div class="row justify-content-between">
                <div class="col-sm-6 col-md-6 col-xl-3">
                    <div class="single-footer-widget footer_1">
                        <a href="/">
                            <img src="/assets/img/logo.jpg" style="width: 100px;" alt="">
                        </a>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-xl-4">
                    <div class="single-footer-widget footer_2">
                        <h4>Newsletter</h4>
                        <p>Stay updated with our latest trends Seed heaven so said place winged over given forth fruit.
                        </p>
                        <form action="#">
                            <div class="form-group">
                                <div class="input-group mb-3">
                                    <input type="text" class="form-control" placeholder='Enter email address'
                                        onfocus="this.placeholder = ''"
                                        onblur="this.placeholder = 'Enter email address'">
                                    <div class="input-group-append">
                                        <button class="btn" type="button"><i class="ti-angle-right"></i></button>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="social_icon">
                            <a href="#"> <i class="ti-facebook"></i> </a>
                            <a href="#"> <i class="ti-twitter-alt"></i> </a>
                            <a href="#"> <i class="ti-instagram"></i> </a>
                            <a href="#"> <i class="ti-skype"></i> </a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-8 col-xl-3">
                    <div class="single-footer-widget footer_3">
                        <h4>Contact</h4>
                        <p>Bourdillon Court Estate, Chevron Drive, Lekki, Lagos State.</p><br>
                        <p> talctechpro202@yahoo.com </p><br>
                        <p> info@talctechng.com</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="copyright_part_text text-center">
                        <div class="row">
                            <div class="col-lg-12">
                                <p class="footer-text m-0">
                                    Copyright &copy;<script>
                                        document.write(new Date().getFullYear());
                                    </script> <a href="#">Taltech</a> - All Rights Reserved
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>



    <script src="/assets/js/jquery-1.12.1.min.js"></script>

    <script src="/assets/js/popper.min.js"></script>

    <script src="/assets/js/bootstrap.min.js"></script>

    <script src="/assets/js/jquery.magnific-popup.js"></script>

    <script src="/assets/js/swiper.min.js"></script>

    <script src="/assets/js/masonry.pkgd.js"></script>

    <script src="/assets/js/owl.carousel.min.js"></script>
    <script src="/assets/js/jquery.counterup.min.js"></script>
    <script src="/assets/js/waypoints.min.js"></script>
    <script src="/assets/js/owl.carousel2.thumbs.min.js"></script>

    <script src="/assets/js/slick.min.js"></script>

    <script src="/assets/js/custom.js"></script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());

        gtag('config', 'UA-23581568-13');
    </script>
</body>

